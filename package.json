{
  "name": "bizmob-lib",
  "version": "1.0.0",
  "description": "bizMOB 라이브러리 파일 관리 프로젝트",
  "main": "public/bizMOB/bizMOB-core.js",
  "types": "src/bizMOB/bizmob.d.ts",
  "scripts": {
    "build": "npm run build:js && npm run build:ts",
    "build:js": "webpack --config webpack.config.js",
    "build:ts": "tsc --build",
    "lint": "eslint public/**/*.js src/**/*.ts",
    "lint:fix": "eslint public/**/*.js src/**/*.ts --fix",
    "clean": "rimraf dist",
    "test": "jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/bizmob-lib.git"
